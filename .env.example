# Create your .env file with:

# LangFuse Credentials
LANGFUSE_SECRET_KEY="<TO BE FILLED>"
LANGFUSE_PUBLIC_KEY="<TO BE FILLED>"
LANGFUSE_BASE_URL="https://us.cloud.langfuse.com"
LANGFUSE_ENABLED=true

# Prompt Names (versioned)
LANGFUSE_PROMPT_CHAT_ROUTER=healthcare-chat-router
LANGFUSE_PROMPT_PROVIDER_SCORER=provider-scoring
LANGFUSE_PROMPT_PATIENT_MESSAGE=patient-engagement-message



# Email Personalization
USE_LANGFUSE_EMAIL_PROMPT=true


# ============================================
# LLM Configuration
# ============================================
# Set provider to "azure" to use Azure OpenAI
ORCHESTRATION_LLM_PROVIDER=azure

# Azure OpenAI Configuration - REPLACE WITH YOUR ACTUAL VALUES
# Get these from Azure Portal -> Your OpenAI Resource -> Keys and Endpoint
ORCHESTRATION_LLM_AZURE_ENDPOINT=https://lalm-mhyzws3j-eastus2.cognitiveservices.azure.com/
ORCHESTRATION_LLM_AZURE_API_KEY="<TO BE FILLED>"
ORCHESTRATION_LLM_AZURE_API_VERSION=2025-03-01-preview
ORCHESTRATION_LLM_MODEL=gpt-5-chat

# Optional LLM Settings
ORCHESTRATION_LLM_TEMPERATURE=0.1
ORCHESTRATION_LLM_MAX_TOKENS=3000
# Demo Authentication
DEMO_PASSWORD=balance
